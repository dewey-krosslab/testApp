    "babel-cli": "^6.26.0",
    "babel-jest": "^26.6.3",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
